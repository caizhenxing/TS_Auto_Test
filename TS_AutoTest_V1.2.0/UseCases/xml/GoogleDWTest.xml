<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./SeleniumUseCase.xsl"?>
<SUITE name="IBM开发者社区 演示测试用例">
	<SETTINGS browser='*iexplore' seleniumServer='127.0.0.1' seleniumPort='4444' application_url='http://www.baidu.com'>
	</SETTINGS>
	<TEST name="在百度中查询test" timeout='1'>
		<STEPS>

			<STEP index="1" type="Type.Input" name="在百度查询输入框，输入test">
				<XPATH>id=kw</XPATH>
				<VALUE>test</VALUE>
			</STEP>

			<STEP index="2" type="Type.Click" name="单击百度一下按钮，执行查询">
				<XPATH>id=su</XPATH>
			</STEP>

			<STEP index='3' type='Type.CaptureScreenshot' name="测试截图函数">
				<VALUE>截图测试XML</VALUE>
			</STEP>

		</STEPS>
	</TEST>
</SUITE> 